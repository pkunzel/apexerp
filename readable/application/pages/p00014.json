   ,{
      "id":"62615034202085543711"
     ,"Identification":{
        "Name":"Validar @"
      }
     ,"Execution":{
        "Sequence":20
      }
     ,"Validation":{
        "Type":"Function Body (returning Boolean)"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Function Body":"DECLARE\n    v_frase VARCHAR2(100) := :P14_EMAIL;\n    v_posicao NUMBER;\nBEGIN\n    -- Verifica se o caractere \u0027@\u0027 est\u00E1 presente na frase\n    v_posicao := INSTR(v_frase, \u0027@\u0027);\n\n    IF v_posicao \u003E 4 THEN\n        return true;\n    ELSE\n        return false;\n    END IF;\nEND;"
       ,"Always Execute":false
      }
     ,"Error":{
        "Error Message":"Seu e-mail precisa possuir \u0027@\u0027"
       ,"Display Location":"Inline with Field and in Notification"
       ,"Associated Item":"P14_EMAIL (63105328767681696384)"
      }
    }
   ,{
      "id":"62615034613222543715"
     ,"Identification":{
        "Name":"Validar .com"
      }
     ,"Execution":{
        "Sequence":70
      }
     ,"Validation":{
        "Type":"Function Body (returning Boolean)"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Function Body":"DECLARE\n    v_frase VARCHAR2(100) := :P14_EMAIL;\n    v_posicao NUMBER;\n    v_com_length NUMBER := LENGTH(\u0027.com\u0027);\n    v_frase_length NUMBER := LENGTH(v_frase);\nBEGIN\n    v_posicao := INSTR(v_frase, \u0027.com\u0027);\n\n    IF v_posicao \u003E 7 AND v_posicao = v_frase_length - v_com_length + 1 THEN\n        return TRUE;\n    else \n    return false;    \n    END IF;\nEND;"
       ,"Always Execute":false
      }
     ,"Error":{
        "Error Message":"E-mail inv\u00E1lido, seu e-mail precisa terminar com \u0027.com\u0027"
       ,"Display Location":"Inline with Field and in Notification"
       ,"Associated Item":"P14_EMAIL (63105328767681696384)"
      }
    }
   ,{
      "id":"64786170606224651516"
     ,"Identification":{
        "Name":"Validar letra Mm"
      }
     ,"Execution":{
        "Sequence":40
      }
     ,"Validation":{
        "Type":"Function Body (returning Boolean)"
       ,"Language":"PL\/SQL"
       ,"PL\/SQL Function Body":"DECLARE\n    v_string VARCHAR2(100) := :P14_SENHA;\n    v_char   CHAR(1);\n    v_pos    NUMBER;\n    v_count number :=0; \n\nBEGIN\n    -- Verifica se h\u00E1 letras mai\u00FAsculas na string\n    FOR i IN 65..90 LOOP -- c\u00F3digo ASCII para letras mai\u00FAsculas\n        v_char := CHR(i);\n        v_pos := INSTR(v_string, v_char);\n        IF v_pos \u003E 0 THEN\n            v_count := v_count +1;\n        END IF;\n    END LOOP;\n\n    -- Verifica se h\u00E1 letras min\u00FAsculas na string\n    FOR i IN 97..122 LOOP -- c\u00F3digo ASCII para letras min\u00FAsculas\n        v_char := CHR(i);\n        v_pos := INSTR(v_string, v_char);\n        IF v_pos \u003E 0 THEN\n            v_count := v_count +1;\n        END IF;\n    END LOOP;\n\n    if v_count = 2 then\n        return true;\n    else \n    return false;\n    end if;\nEND;"
       ,"Always Execute":false
      }
     ,"Error":{
        "Error Message":"Senha inv\u00E1lida, deve conter uma letra mai\u00FAscula e min\u00FAscula"
       ,"Display Location":"Inline with Field and in Notification"
       ,"Associated Item":"P14_SENHA (63105331175245696385)"
      }
